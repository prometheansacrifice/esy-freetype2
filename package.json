{
  "name": "esy-freetype-2.10.0",
  "version": "0.0.000",
  "description": "freetype-2.10.0 packaged for esy",
  "esy": {
    "buildsInSource": true,
    "exportedEnv": {
      "PKG_CONFIG_PATH": {
        "scope": "global",
        "val": "#{self.lib / 'pkgconfig'}:$PKG_CONFIG_PATH"
      }
    },
    "build": [
      [
        "chmod",
        "755",
        "./configure"
      ],
      [
        "bash",
        "-c",
        "./configure --prefix=#{self.install} #{os == 'windows' ? '--host x86_64-w64-mingw32' : ''}"
      ],
      [
        "make"
      ],
      [
        "make",
        "install"
      ]
    ]
  },
  "dependencies": {
    "esy-zlib": "esy-packages/esy-zlib#404929fd8b7ed83ed6a528d751840faff957b4b3",
    "esy-libpng": "esy-packages/esy-libpng#d846df4a8547b3299c7627773008bd97543d7048",
    "esy-harfbuzz": "esy-packages/esy-harfbuzz#4c8e602a063b10d6b81e3c7cc68cb53f1976470c",
    "esy-bzip2": "esy-packages/esy-bzip2#048f314334ae2e63b24082a7e4cfca4985b04028",
    "esy-python": "esy-packages/esy-python#61d8c1a8a1e6ad6e73991e20ba0570e5c60ff1da"
  },
  "resolutions": {
    "esy-zlib": "esy-packages/esy-zlib#404929fd8b7ed83ed6a528d751840faff957b4b3"
  }
}
